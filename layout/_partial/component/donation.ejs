<% if (theme.donation.show) { %>
  <div class="text-center donation">
    <div class="inner-donation">
      <span class="btn-donation"><%= theme.donation.btn_text %></span>
      <div class="donation-body">
        <div class="tip text-center"><%= theme.donation.tip %></div>
        <ul class="theme.donation.items.length">
        <% theme.donation.items.forEach(function(item, i){ %>
          <li class="item">
            <% if( theme.yundashang.isShow ) { %>
              <div name="dashmain" id="<%= theme.yundashang.id %>" class="dash-main-4 879895-1"></div>
              <script type="text/javascript" charset="utf-8" src="https://www.dashangcloud.com/static/ds-2.0.js"></script>
            <% } else { %>
              <img src="<%= item.imgUrl %>" alt="">
            <% } %>
          </li>
        <% }) %>
        </ul>
      </div>
    </div>
  </div>
<% } %>
